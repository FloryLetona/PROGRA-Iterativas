using System;
class program
{
static void Main()
{
int num;

Console.WriteLine("CALCULAR EL AREA DE LOS CIRCULOS");
Console.WriteLine("INGRESA EL NUMERO DE CIRCULOS A ENCONTRAR EL AREA: ");

num = Convert.ToInt32(Console.ReadLine());

int multi=0;
int i; 
for (i = 1; i <= 100; i++) { multi = multi + num; Console.WriteLine(  "EL AREA DEL CIRCULO  "   +i+   " ES  ="  +(3.1416*(i*i))); } Console.ReadKey(); } }
